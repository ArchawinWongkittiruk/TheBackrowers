<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="outputRoot" prefHeight="319.0" prefWidth="600.0" stylesheets="@../Styling/GUI.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.OutputController">
   <children>
      <TextArea fx:id="outputText" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="182.0" prefWidth="560.0" VBox.vgrow="ALWAYS" />
      <BorderPane>
         <center>
            <Label fx:id="fileSaved" prefHeight="17.0" prefWidth="122.0" BorderPane.alignment="CENTER" />
         </center>
         <left>
            <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearTextField" prefHeight="26.0" prefWidth="78.0" text="Clear" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveFile" prefHeight="26.0" prefWidth="78.0" text="Save" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
